{% extends "::main.html.twig" %}

{% block title %}CodersLabBundle:Tweet:showAll{% endblock %}

{% block body %}

    {% for course in allCourses %}
        <p>
            {{ course.subject.name }}<br>
            {{ course.location }},  {{ course.weekday }} {{ course.starttime}} - {{ course.endtime}}
            {% for mentor in course.mentors %}
                {{ mentor.name }}
            {% endfor %}
    {% endfor %}
    
{% endblock %}
